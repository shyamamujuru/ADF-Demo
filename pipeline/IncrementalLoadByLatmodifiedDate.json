{
	"name": "IncrementalLoadByLatmodifiedDate",
	"properties": {
		"activities": [
			{
				"name": "Copy_twe",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "FileIncrementalLoad/Source/"
					},
					{
						"name": "Destination",
						"value": "FileIncrementalLoad/Destination/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureDataLakeStoreSource",
						"recursive": false
					},
					"sink": {
						"type": "AzureDataLakeStoreSink",
						"copyBehavior": "PreserveHierarchy"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_twe",
						"type": "DatasetReference",
						"parameters": {
							"cw_modifiedDatetimeStart": "@pipeline().parameters.windowStart",
							"cw_modifiedDatetimeEnd": "@pipeline().parameters.windowEnd"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_twe",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"windowStart": {
				"type": "String"
			},
			"windowEnd": {
				"type": "String"
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}